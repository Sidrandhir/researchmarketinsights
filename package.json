{
  "name": "market-research-insights",
  "version": "1.0.0",
  "description": "Comprehensive market research platform with multi-language support",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force && npm run db:seed",
    "analyze": "ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@hookform/resolvers": "^3.3.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.292.0",
    "multer": "^1.4.5-lts.1",
    "next": "^15.4.6",
    "next-auth": "^4.24.11",
    "next-seo": "^6.4.0",
    "next-sitemap": "^4.2.0",
    "nodemailer": "^6.9.0",
    "postcss": "^8.4.0",
    "prisma": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.0",
    "recharts": "^2.8.0",
    "sharp": "^0.32.0",
    "stripe": "^14.0.0",
    "swiper": "^11.2.10",
    "swr": "^2.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^1.4.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
